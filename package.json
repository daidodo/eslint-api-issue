{
  "name": "eslint-api-issue",
  "version": "1.0.7",
  "description": "",
  "main": "dist/main.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist/",
    "prepublishOnly": "npm run clean && npm i && webpack",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daidodo/eslint-api-issue.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daidodo/eslint-api-issue/issues"
  },
  "homepage": "https://github.com/daidodo/eslint-api-issue#readme",
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "no-unused-vars": [
        "warn",
        {
          "varsIgnorePattern": "^_",
          "argsIgnorePattern": "^_",
          "ignoreRestSiblings": true
        }
      ]
    },
    "ignorePatterns": "**/xxx.js"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "eslint": "^8.11.0",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "@typescript-eslint/parser": "^5.15.0"
  }
}
